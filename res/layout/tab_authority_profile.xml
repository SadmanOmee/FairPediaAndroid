<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
    <ImageView
        android:id="@+id/imgview_aupropic"
        android:layout_width="332dp"
        android:layout_height="335dp"
        app:srcCompat="@drawable/rpic"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/imgbtn_aupropicchange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        app:srcCompat="@android:drawable/ic_menu_camera"
        android:layout_below="@+id/imgview_aupropic"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Change Profile Picture"
        android:layout_below="@+id/imgbtn_aupropicchange"
        android:layout_centerHorizontal="true" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView5"
                android:layout_marginLeft="12dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="48dp"
                android:text="Account Name :"
                android:textSize="24sp" />

    <TextView
        android:id="@+id/txtview_authoritydisplayname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView7"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_toEndOf="@+id/imgbtn_aupropicchange"
        android:layout_toRightOf="@+id/imgbtn_aupropicchange"
        android:text="TextView"
        android:textSize="24sp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView7"
                android:layout_alignStart="@+id/textView7"
                android:layout_below="@+id/textView7"
                android:layout_marginTop="40dp"
                android:text="Email :"
                android:textSize="24sp" />

    <TextView
        android:id="@+id/txtview_authoritydisplayemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24sp"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignLeft="@+id/txtview_authoritydisplayname"
        android:layout_alignStart="@+id/txtview_authoritydisplayname" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView10"
                android:layout_alignStart="@+id/textView10"
                android:layout_below="@+id/textView10"
                android:layout_marginTop="38dp"
                android:text="Stall Name :"
                android:textSize="24sp" />

    <TextView
        android:id="@+id/txtview_authoritydisplaystallname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24sp"
        android:layout_alignBaseline="@+id/textView20"
        android:layout_alignBottom="@+id/textView20"
        android:layout_alignLeft="@+id/txtview_authoritydisplayemail"
        android:layout_alignStart="@+id/txtview_authoritydisplayemail" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView20"
                android:layout_alignStart="@+id/textView20"
                android:layout_below="@+id/textView20"
                android:layout_marginTop="43dp"
                android:text="Stall Type :"
                android:textSize="24sp" />

    <TextView
        android:id="@+id/txtview_authoritydisplaystalltype"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView28"
        android:layout_alignLeft="@+id/txtview_authoritydisplaystallname"
        android:layout_alignStart="@+id/txtview_authoritydisplaystallname"
        android:text="TextView"
        android:textSize="24sp" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView28"
                android:layout_alignStart="@+id/textView28"
                android:layout_below="@+id/textView28"
                android:layout_marginTop="40dp"
                android:text="Fair Frquency/Year:"
                android:textSize="24sp" />

    <TextView
        android:id="@+id/txtview_authoritydisplayfairfreq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView30"
        android:layout_alignLeft="@+id/txtview_authoritydisplaystalltype"
        android:layout_alignStart="@+id/txtview_authoritydisplaystalltype"
        android:text="TextView"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:text="TextView"
        android:textSize="24sp"
        android:layout_below="@+id/textView30"
        android:layout_alignLeft="@+id/textView30"
        android:layout_alignStart="@+id/textView30" />

    <TextView
        android:id="@+id/txtview_authoritydisplayaddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24sp"
        android:layout_alignBaseline="@+id/textView32"
        android:layout_alignBottom="@+id/textView32"
        android:layout_alignLeft="@+id/txtview_authoritydisplayfairfreq"
        android:layout_alignStart="@+id/txtview_authoritydisplayfairfreq" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView32"
                android:layout_alignStart="@+id/textView32"
                android:layout_below="@+id/textView32"
                android:layout_marginTop="46dp"
                android:text="Zip Code :"
                android:textSize="24sp" />

    <TextView
        android:id="@+id/txtview_authoritydisplayzipcode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24sp"
        android:layout_alignBaseline="@+id/textView34"
        android:layout_alignBottom="@+id/textView34"
        android:layout_alignLeft="@+id/txtview_authoritydisplayaddress"
        android:layout_alignStart="@+id/txtview_authoritydisplayaddress" />

            <TextView
                android:id="@+id/textView36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView34"
                android:layout_alignStart="@+id/textView34"
                android:layout_below="@+id/textView34"
                android:layout_marginTop="50dp"
                android:text="Contact No :"
                android:textSize="24sp" />

    <TextView
        android:id="@+id/txtview_authoritydisplaymobileno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView36"
        android:layout_alignLeft="@+id/txtview_authoritydisplayzipcode"
        android:layout_alignStart="@+id/txtview_authoritydisplayzipcode"
        android:text="TextView"
        android:textSize="24sp" />
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>